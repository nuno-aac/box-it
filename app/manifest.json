{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Box it",
  "description": "Extension for debugging",
  "author": "Nuno Cunha <naac.1999@gmail.com>",
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup/index.html"
  }
}
